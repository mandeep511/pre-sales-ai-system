    "@types/ws": "^8.5.12",
    "bcrypt": "^5.1.1",
    "connect-mongo": "^5.1.0",
    "connect-redis": "^9.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.18.2",
    "helmet": "^8.1.0",
    "httpdispatcher": "^2.2.0",
